{
  "name": "event4u/laravel-data-helpers",
  "description": "A universal mapper for Arrays, DTOs, Eloquent, Collections, JSON & XML with dot-notation, wildcards, templates, and hooks.",
  "license": "proprietary",
  "type": "library",
  "keywords": [
    "helpers",
    "dot-notations",
    "accassor",
    "mutator",
    "mapper"
  ],
  "authors": [
    {
      "name": "Mathias Berg",
      "email": "m.berg@event4u.app"
    }
  ],
  "require": {
    "php": "^8.2 || ^8.3 || ^8.4 || ^8.5",
    "composer-plugin-api": "^2.0",
    "laravel/framework": "^8 || ^9 || ^10 || ^11 || ^12"
  },
  "require-dev": {
    "composer/composer": "^2",
    "pestphp/pest": "^3"
  },
  "suggest": {
    "illuminate/support": "Required for Laravel Collection and Arrayable support (^8 || ^9 || ^10 || ^11 || ^12)",
    "illuminate/database": "Required for Eloquent Model support (^8 || ^9 || ^10 || ^11 || ^12)"
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "conflict": {},
  "autoload": {
    "psr-4": {
      "event4u\\DataHelpers\\": "src/"
    },
    "files": [
      "src/Polyfills/bootstrap.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/",
      "Examples\\": "examples/"
    }
  },
  "bin": [],
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  }
}
