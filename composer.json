{
  "name": "event4u/data-helpers",
  "description": "Framework-agnostic helpers for structured data access, mutation, and mapping using dot-notation paths with wildcards. Works with Laravel, Symfony/Doctrine, or standalone PHP.",
  "license": "proprietary",
  "type": "library",
  "keywords": [
    "data",
    "accessor",
    "mutator",
    "mapper",
    "laravel",
    "symfony",
    "doctrine",
    "php"
  ],
  "authors": [
    {
      "name": "matze4u",
      "homepage": "https://event4u.app",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.2",
    "composer-plugin-api": "^2.0"
  },
  "require-dev": {
    "composer/composer": "^2.0",
    "doctrine/collections": "^2.0|^3.0",
    "doctrine/orm": "^2.0|^3.0",
    "ergebnis/phpstan-rules": "^2.12",
    "illuminate/database": "^9.0|^10.0|^11.0",
    "illuminate/http": "^9.0|^10.0|^11.0",
    "illuminate/support": "^9.0|^10.0|^11.0",
    "jangregor/phpstan-prophecy": "^2.2",
    "pestphp/pest": "^2.0|^3.0",
    "phpat/phpat": "^0.12.0",
    "phpstan/phpstan": "^2.1",
    "phpstan/phpstan-mockery": "^2.0",
    "phpstan/phpstan-phpunit": "^2.0",
    "rector/rector": "^2.1",
    "spaze/phpstan-disallowed-calls": "^4.6",
    "symplify/coding-standard": "^12.4",
    "symplify/easy-coding-standard": "^12.6",
    "timeweb/phpstan-enum": "^4.0",
    "phpbench/phpbench": "^1.4",
    "symfony/http-kernel": "^6.0|^7.0",
    "symfony/http-foundation": "^6.0|^7.0",
    "graham-campbell/result-type": "^1.1",
    "vlucas/phpdotenv": "^5.6",
    "symfony/dependency-injection": "^7.3",
    "symfony/config": "^7.3",
    "symfony/yaml": "^7.3",
    "symfony/cache": "^7.3",
    "illuminate/cache": "^11.46"
  },
  "repositories": [],
  "suggest": {},
  "minimum-stability": "stable",
  "prefer-stable": true,
  "conflict": {},
  "autoload": {
    "psr-4": {
      "event4u\\DataHelpers\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/",
      "Examples\\": "examples/"
    }
  },
  "bin": [],
  "scripts": {
    "composer:update": "composer update php --prefer-dist --no-interaction -W 2>&1",
    "test": "vendor/bin/pest --no-coverage",
    "test:coverage": "vendor/bin/pest",
    "test:laravel9": [
      "./scripts/test-with-versions.sh --laravel 9",
      "@composer:update"
    ],
    "test:laravel10": [
      "./scripts/test-with-versions.sh --laravel 10",
      "@composer:update"
    ],
    "test:laravel11": [
      "./scripts/test-with-versions.sh --laravel 11",
      "@composer:update"
    ],
    "test:symfony6": [
      "./scripts/test-with-versions.sh --symfony 6",
      "@composer:update"
    ],
    "test:symfony7": [
      "./scripts/test-with-versions.sh --symfony 7",
      "@composer:update"
    ],
    "test:doctrine2": [
      "./scripts/test-with-versions.sh --doctrine 2",
      "@composer:update"
    ],
    "test:doctrine3": [
      "./scripts/test-with-versions.sh --doctrine 3",
      "@composer:update"
    ],
    "test:all-laravel": [
      "@test:laravel9",
      "@test:laravel10",
      "@test:laravel11"
    ],
    "test:all-symfony": [
      "@test:symfony6",
      "@test:symfony7"
    ],
    "test:all-doctrine": [
      "@test:doctrine2",
      "@test:doctrine3"
    ],
    "test:all": [
      "@test:all-laravel",
      "@test:all-symfony",
      "@test:all-doctrine"
    ],
    "phpstan": "vendor/bin/phpstan analyse --memory-limit=512M",
    "phpstan:baseline": "vendor/bin/phpstan analyse --memory-limit=512M --generate-baseline",
    "phpstan:clear-cache": "vendor/bin/phpstan --clear-result-cache",
    "ecs": "vendor/bin/ecs check",
    "ecs:fix": "vendor/bin/ecs check --fix",
    "rector": "vendor/bin/rector process --dry-run",
    "rector:fix": "vendor/bin/rector process",
    "refactor": [
      "@ecs",
      "@rector"
    ],
    "refactor:fix": [
      "@ecs:fix",
      "@rector:fix"
    ],
    "quality": [
      "@ecs",
      "@rector",
      "@phpstan",
      "@test"
    ],
    "quality:fix": [
      "@ecs:fix",
      "@rector:fix",
      "@phpstan",
      "@test"
    ]
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "event4u\\DataHelpers\\Laravel\\LaravelMappedModelServiceProvider",
        "event4u\\DataHelpers\\Laravel\\LaravelConfigDataHelpersServiceProvider"
      ]
    },
    "symfony": {
      "require": "^6.0|^7.0",
      "bundles": {
        "event4u\\DataHelpers\\Symfony\\SymfonyMappedModelResolver": [
          "all"
        ],
        "event4u\\DataHelpers\\Symfony\\SymfonyDataHelpersBundle": [
          "all"
        ]
      }
    }
  }
}
