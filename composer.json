{
  "name": "event4u/data-helpers",
  "description": "Framework-agnostic helpers for structured data access, mutation, and mapping using dot-notation paths with wildcards. Works with Laravel, Symfony/Doctrine, or standalone PHP.",
  "license": "proprietary",
  "type": "library",
  "keywords": [
    "data",
    "accessor",
    "mutator",
    "mapper",
    "laravel",
    "symfony",
    "doctrine",
    "php"
  ],
  "authors": [
    {
      "name": "matze4u",
      "homepage": "https://event4u.app",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.2",
    "composer-plugin-api": "^2.0",
    "ext-simplexml": "*"
  },
  "require-dev": {
    "composer/composer": "^2.0",
    "doctrine/collections": "^2.0|^3.0",
    "doctrine/orm": "^2.0|^3.0",
    "ergebnis/phpstan-rules": "^2.12",
    "graham-campbell/result-type": "^1.1",
    "illuminate/cache": "^9.0|^10.0|^11.0",
    "illuminate/database": "^9.0|^10.0|^11.0",
    "illuminate/http": "^9.0|^10.0|^11.0",
    "illuminate/support": "^9.0|^10.0|^11.0",
    "jangregor/phpstan-prophecy": "^2.2",
    "nesbot/carbon": "^2.72|^3.0",
    "pestphp/pest": "^2.0|^3.0",
    "phpat/phpat": "^0.12.0",
    "phpbench/phpbench": "^1.4",
    "phpstan/phpstan": "^2.0",
    "phpstan/phpstan-mockery": "^2.0",
    "phpstan/phpstan-phpunit": "^2.0",
    "rector/rector": "^2.1",
    "spaze/phpstan-disallowed-calls": "^4.6",
    "symfony/cache": "^6.0|^7.0",
    "symfony/config": "^6.0|^7.0",
    "symfony/dependency-injection": "^6.0|^7.0",
    "symfony/http-foundation": "^6.0|^7.0",
    "symfony/http-kernel": "^6.0|^7.0",
    "symfony/yaml": "^6.0|^7.0",
    "symplify/coding-standard": "^12.4",
    "symplify/easy-coding-standard": "^12.6",
    "timeweb/phpstan-enum": "^4.0",
    "vlucas/phpdotenv": "^5.6",
    "symfony/serializer": "^6.0|^7.0",
    "symfony/property-info": "^6.0|^7.0",
    "symfony/property-access": "^6.0|^7.0",
    "symfony/validator": "^6.0|^7.0",
    "fakerphp/faker": "^1.24"
  },
  "repositories": [],
  "suggest": {
    "nesbot/carbon": "Required for EnvHelper::carbon() method to parse date/time environment variables"
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "conflict": {},
  "autoload": {
    "psr-4": {
      "event4u\\DataHelpers\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "TestsE2E\\": "tests-e2e/",
      "Tests\\": "tests/",
      "Examples\\": "examples/"
    },
    "files": [
      "tests/helpers-laravel.php"
    ]
  },
  "bin": [],
  "scripts": {},
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "extra": {
    "laravel": {
      "require": "^9.0|^10.0|^11.0",
      "providers": [
        "event4u\\DataHelpers\\Frameworks\\Laravel\\DataHelpersServiceProvider"
      ]
    },
    "symfony": {
      "require": "^6.0|^7.0",
      "bundles": {
        "event4u\\DataHelpers\\Frameworks\\Symfony\\DataHelpersBundle": [
          "all"
        ]
      }
    }
  }
}
