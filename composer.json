{
  "name": "event4u/data-helpers",
  "description": "Framework-agnostic helpers for structured data access, mutation, and mapping using dot-notation paths with wildcards. Works with Laravel, Symfony/Doctrine, or standalone PHP.",
  "license": "proprietary",
  "type": "library",
  "keywords": [
    "data",
    "accessor",
    "mutator",
    "mapper",
    "laravel",
    "symfony",
    "doctrine",
    "php"
  ],
  "authors": [
    {
      "name": "Mathias Berg",
      "email": "m.berg@event4u.app"
    }
  ],
  "require": {
    "php": "^8.2",
    "composer-plugin-api": "^2.0"
  },
  "require-dev": {
    "composer/composer": "^2",
    "pestphp/pest": "^3",
    "illuminate/support": "^8",
    "doctrine/collections": "^1.6 || ^2",
    "doctrine/orm": "^2.10 || ^3"
  },
  "repositories": [],
  "suggest": {
    "illuminate/support": "Required for Laravel Collection and Arrayable support (^8.0)",
    "illuminate/database": "Required for Eloquent Model support (^8.0)",
    "doctrine/collections": "Required for Doctrine Collection support (^1.6)",
    "doctrine/orm": "Required for Doctrine Entity support (^2.10)"
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "conflict": {},
  "autoload": {
    "psr-4": {
      "event4u\\DataHelpers\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/",
      "Examples\\": "examples/"
    }
  },
  "bin": [],
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  }
}
