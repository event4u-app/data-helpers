{
  "name": "event4u/data-helpers",
  "description": "Framework-agnostic helpers for structured data access, mutation, and mapping using dot-notation paths with wildcards. Works with Laravel, Symfony/Doctrine, or standalone PHP.",
  "license": "proprietary",
  "type": "library",
  "keywords": [
    "data",
    "accessor",
    "mutator",
    "mapper",
    "laravel",
    "symfony",
    "doctrine",
    "php"
  ],
  "authors": [
    {
      "name": "matze4u",
      "homepage": "https://event4u.app",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.2",
    "composer-plugin-api": "^2.0"
  },
  "require-dev": {
    "composer/composer": "^2.0",
    "doctrine/collections": "^2.0",
    "doctrine/orm": "^3.0",
    "ergebnis/phpstan-rules": "^2.12",
    "illuminate/database": "^11.0",
    "illuminate/support": "^11.0",
    "jangregor/phpstan-prophecy": "^2.2",
    "pestphp/pest": "^3.0",
    "phpat/phpat": "^0.12.0",
    "phpstan/phpstan": "^2.1",
    "phpstan/phpstan-mockery": "^2.0",
    "phpstan/phpstan-phpunit": "^2.0",
    "rector/rector": "^2.1",
    "spaze/phpstan-disallowed-calls": "^4.6",
    "symplify/coding-standard": "^12.4",
    "symplify/easy-coding-standard": "^12.6",
    "timeweb/phpstan-enum": "^4.0",
    "phpbench/phpbench": "^1.4"
  },
  "repositories": [],
  "suggest": {
    "illuminate/support": "Required for Laravel Collection and Arrayable support (^11.0)",
    "illuminate/database": "Required for Eloquent Model support (^11.0)",
    "doctrine/collections": "Required for Doctrine Collection support (^1.6 || ^2.0)",
    "doctrine/orm": "Required for Doctrine Entity support (^2.10 || ^3.0)"
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "conflict": {},
  "autoload": {
    "psr-4": {
      "event4u\\DataHelpers\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/",
      "Examples\\": "examples/"
    }
  },
  "bin": [],
  "scripts": {
    "test": "vendor/bin/pest --no-coverage",
    "test:coverage": "vendor/bin/pest",
    "phpstan": "vendor/bin/phpstan analyse --memory-limit=512M",
    "phpstan:baseline": "vendor/bin/phpstan analyse --memory-limit=512M --generate-baseline",
    "ecs": "vendor/bin/ecs check",
    "ecs:fix": "vendor/bin/ecs check --fix",
    "rector": "vendor/bin/rector process --dry-run",
    "rector:fix": "vendor/bin/rector process",
    "quality": [
      "@phpstan",
      "@ecs",
      "@test"
    ],
    "quality:fix": [
      "@ecs:fix",
      "@rector:fix",
      "@test"
    ]
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "event4u\\DataHelpers\\Integration\\LaravelMappedModelServiceProvider"
      ]
    },
    "symfony": {
      "require": "^6.0|^7.0"
    }
  }
}
